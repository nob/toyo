<script type="text/javascript">
        /* 地図の初期化 */
        function initialize() {
            /* ★緯度・経度：日本 */
            // var latlng=new google.maps.LatLng(35.624041,139.721417);
            var latlng=new google.maps.LatLng(35.688106,139.696902);

            /* ★★地図のオプション設定 */
            var myOptions={
                /*初期のズーム レベル */
                zoom: 16,
                /* 地図の中心点 */
                center: latlng,
                /* 地図タイプ */
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            /* ★★★地図オブジェクト */
            var map=new google.maps.Map(document.getElementById("map"), myOptions);
            /* マーカーの位置 */
            var setposition=new google.maps.LatLng(35.688106,139.696902);
            /* アイコン */
            var image=new google.maps.MarkerImage(
                'http://www.mvpro.jp/dental/tooth1.png',
                new google.maps.Size(42,35),
                new google.maps.Point(0,0),
                new google.maps.Point(1,23)
            );
            /* アイコンの影 */
            var shadow=new google.maps.MarkerImage(
                'http://www.mvpro.jp/dental/tooth2.png',
                new google.maps.Size(42,35),
                new google.maps.Point(0,0),
                new google.maps.Point(1,23)
            );
            /* マーカーオブジェクト */
            var marker=new google.maps.Marker({
                position: setposition,
                map: map,
                icon: image,
                shadow: shadow
            });

               //★★★★吹き出しを作成します
                var contentString = '<div style="width: 230px; padding-right: 10px">西新宿歯科TOYOクリニック<br>東京都新宿区西新宿1-19-7岡崎ビル3Ｆ<br><br>03-5324-6789<\/div>';
                var infowindow = new google.maps.InfoWindow({
                    content: contentString  //吹き出し内コメント
                });



                //クリックしたときに吹き出しがオープンするイベントを定義します
                google.maps.event.addListener(marker, 'click', function() {
                    infowindow.open(map,marker);
                });
        }

        /* ロード時に初期化 */
        google.maps.event.addDomListener(window, 'load', initialize);
</script>
